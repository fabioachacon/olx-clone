oggle');\n    }\n\n\n    return (\n        <PageContainer>\n          <PageTitle>Signup</PageTitle>\n          <PageArea>\n              {error &&\n                 <ErrorMessage>{error}</ErrorMessage>\n              }\n              <form onSubmit={handlerSubmit}>\n                <label className=\"area\" htmlFor=\"\">\n                   <div className=\"area--title\">Nome Completo</div>\n                   <div className=\"area--input\">\n                       <input \n                          type=\"text\" \n                          disabled={disabled}\n                          value={name}\n                          required\n                          onChange={e => setName(e.target.value)}\n                        />\n                   </div>\n                 </label>\n                 <label className=\"area\" htmlFor=\"\">\n                   <div className=\"area--title\">Estado</div>\n                   <div className=\"area--input\">\n                       <select onChange={e => setStateLoc(e.target.value)} value={stateLoc} required name=\"\" id=\"\">\n                          <option ></option>\n                          {stateList.map((item, key) => (\n                              <option key={key} value={item.id}>{item.name}</option>\n                          ))}\n                       </select>\n                   </div>\n                 </label>\n                 <label className=\"area\" htmlFor=\"\">\n                   <div className=\"area--title\">E-mail</div>\n                   <div className=\"area--input\">\n                       <input \n                          type=\"email\" \n                          disabled={disabled}\n                          value={email}\n                          required\n                          onChange={e => setEmail(e.target.value)}\n                           />\n                   </div>\n                 </label>\n                 <label className=\"area\" htmlFor=\"\">\n                   <div className=\"area--title\">Senha</div>\n                   <div className=\"area--input\">\n                       <input \n                         type=\"password\" \n                         disabled={disabled} \n                         value={password}\n                         required\n                         onChange={e => setPassword(e.target.value)}\n                        />\n                   </div>\n                 </label>\n                 <label className=\"area\" htmlFor=\"\">\n                   <div className=\"area--title\">Confirmar Senha</div>\n                   <div className=\"area--input\">\n                       <input \n                         type=\"password\" \n                         disabled={disabled} \n                         value={confirmPassword}\n                         required\n                         onChange={e => setConfirmPassword(e.target.value)}\n                        />\n                   </div>\n                 </label>\n                 <label className=\"area\" htmlFor=\"\">\n                   <div className=\"area--title\"></div>\n                   <div className=\"area--input\">\n                       <button disabled={disabled} onMouseOver={toggleOverHandler} onMouseLeave={toggleLeaveHandler}>\n                         Cadastrar\n                         <div className=\"hover\"></div>\n                       </button>\n                   </div>\n                 </label>\n              </form>\n          </PageArea>\n        </PageContainer>\n    );\n}\n\nexport default Page;\n","/home/fabio/Documents/React Course/OLX Replica/olx/frontend/src/pages/Signup/styled.js",[],"/home/fabio/Documents/React Course/OLX Replica/olx/frontend/src/pages/Home/styled.js",[],"/home/fabio/Documents/React Course/OLX Replica/olx/frontend/src/components/partials/AdItem/index.js",[],"/home/fabio/Documents/React Course/OLX Replica/olx/frontend/src/components/partials/AdItem/styled.js",[],"/home/fabio/Documents/Reac